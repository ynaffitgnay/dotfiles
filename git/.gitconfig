[User]
	name = Tiffany Yang
	email = tiffanyyang@utexas.edu
[alias]
	st = status -sb -uall
	lg = log --decorate --pretty=oneline --abbrev-commit --graph
	undocommit = reset --soft HEAD^
	undopush = push -f origin HEAD^:master
[core]
	excludesfile = ~/.gitignore_global
[push]
	default = simple
[pull]
	rebase = true
[diff]
	colorMoved = zebra
